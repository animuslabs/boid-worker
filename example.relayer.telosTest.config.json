{
  "$schema": "https://json.schemastore.org/pm2-ecosystem.json",
  "apps": [
    {
      "name": "relayer-server-telosTest",
      "cwd": "./dist",
      "script": "./servers/relayer.js",
      "autorestart": true,
      "cron_restart": "0 0 * * *",
      "env": {
        "CHAIN": "telosTest",
        "LOGLEVEL": "DEBUG",
        "NODE_OPTIONS": "--enable-source-maps"
      },
      "args": [
        "--color"
      ]
    }
  ]
}
