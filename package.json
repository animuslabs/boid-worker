{
  "name": "boid-worker",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias -f",
    "dev": "nodemon -w ./src -e .ts --exec \"yarn build\" ",
    "db:migrate": "edgedb migration create && edgedb migrate && edgeql-js",
    "db:reset": "edgedb instance destroy -I boid_worker --force && edgedb project init --non-interactive && edgeql-js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src/**/* --fix",
    "relayer": "cd dist && LOGLEVEL=DEBUG node ./servers/relayer.js",
    "relayer:dev": "cd dist && LOGLEVEL=DEBUG nodemon -d 3 ./servers/relayer.js",
    "cpRouter": "cp ./dist/servers/relayer.d.ts ../boid-web/src/lib/types "
  },
  "license": "ISC",
  "devDependencies": {
    "@digitak/tsc-esm": "^3.1.4",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.6.1",
    "tsc-alias": "^1.7.0",
    "typescript": "*"
  },
  "dependencies": {
    "@greymass/eosio": "^0.6.3",
    "@prisma/client": "^4.6.1",
    "@trpc/server": "^10.0.0-proxy-beta.15",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.14",
    "axios": "^0.27.2",
    "cache-manager": "^4.1.0",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "edgedb": "^0.22.7",
    "express": "^4.18.1",
    "express-blacklist": "^1.0.3",
    "express-rate-limit": "^6.6.0",
    "fs-extra": "^10.1.0",
    "greenlock-proxy": "^0.2.0",
    "greenlock-proxy-handler": "^0.0.1",
    "uint8arraylist": "2.3.2",
    "ipfs-core": "^0.16.1",
    "ipfs-http-client": "^58.0.1",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "ms": "^2.1.3",
    "multiformats": "^9.9.0",
    "node-fetch": "^3.2.10",
    "node-mailjet": "^5.1.1",
    "prisma-client": "^0.0.0",
    "tozod": "^3.0.0",
    "trpc-openapi": "^1.0.0-alpha.2",
    "ws": "^8.9.0",
    "zod": "^3.19.1"
  }
