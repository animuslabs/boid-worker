{
  "$schema": "https://json.schemastore.org/pm2-ecosystem.json",
  "apps": [
    {
      "name": "boid-worker-fah-saveTeamMembers",
      "cwd": "./dist",
      "script": "./jobs/fah/saveTeamMembers.js",
      "autorestart": false,
      "cron_restart": "*/15 * * * *",
      "env": {
        "CHAIN": "telos",
        "LOGLEVEL": "DEBUG",
        "NODE_NO_WARNINGS": "1",
        "NODE_OPTIONS": "--experimental-specifier-resolution=node --enable-source-maps"
      },
      "args": [
        "--color"
      ]
    },
    {
      "name": "boid-worker-fah-doReport",
      "cwd": "./dist",
      "script": "./jobs/fah/doReports.js",
      "autorestart": false,
      "cron_restart": "*/15 * * * *",
      "env": {
        "CHAIN": "telos",
        "LOGLEVEL": "DEBUG",
        "NODE_NO_WARNINGS": "1",
        "NODE_OPTIONS": "--experimental-specifier-resolution=node --enable-source-maps"
      },
      "args": [
        "--color"
      ]
    },
    {
      "name": "boid-worker-sys-saveBoidIds",
      "cwd": "./dist",
      "script": "./jobs/sys/saveBoidIds.js",
      "autorestart": false,
      "cron_restart": "*/30 * * * *",
      "env": {
        "CHAIN": "telos",
        "LOGLEVEL": "DEBUG",
        "NODE_NO_WARNINGS": "1",
        "NODE_OPTIONS": "--experimental-specifier-resolution=node --enable-source-maps"
      },
      "args": [
        "--color"
      ]
    },
    {
      "name": "boid-worker-sys-handleBoidAccounts",
      "cwd": "./dist",
      "script": "./jobs/sys/handleBoidAccounts.js",
      "autorestart": false,
      "cron_restart": "10 */1 * * *",
      "env": {
        "CHAIN": "telos",
        "LOGLEVEL": "DEBUG",
        "NODE_NO_WARNINGS": "1",
        "NODE_OPTIONS": "--experimental-specifier-resolution=node --enable-source-maps"
      },
      "args": [
        "--color"
      ]
    }
  ]
}
